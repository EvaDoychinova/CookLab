{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "dist/umd/popper.js",
        "dist/umd/popper.min.js"
      ]
    },
    {
      "library": "bootstrap@4.5.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "select2@4.1.0-beta.1",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "dist/css/select2.css",
        "dist/css/select2.min.css",
        "dist/js/select2.js",
        "dist/js/select2.min.js"
      ]
    },
    {
      "library": "jquery-easing@0.0.1",
      "destination": "wwwroot/lib/jquery-easing/",
      "files": [
        "dist/jquery.easing.1.3.umd.js",
        "dist/jquery.easing.1.3.umd.min.js"
      ]
    },
    {
      "library": "magnific-popup@1.1.0",
      "destination": "wwwroot/lib/magnific-popup/",
      "files": [
        "dist/magnific-popup.css",
        "dist/magnific-popup.min.css",
        "dist/jquery.magnific-popup.js",
        "dist/jquery.magnific-popup.min.js"
      ]
    },
    {
      "library": "@fortawesome/fontawesome-free@5.15.1",
      "destination": "wwwroot/lib/fontawesome/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "js/all.js",
        "js/all.min.js",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-brands-400.woff2"
      ]
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/",
      "files": [
        "qrcode.js",
        "qrcode.min.js"
      ]
    }
  ]
}
